@model Actividad1U2.Models.ViewModels.EspeciesViewModel
@{
    Layout = "Layout";
}
<h1>
    <img src="~/clases/@(Model.Id).png" /> @Model.NombreEspecie
</h1>

    <div class="listaEspecies2">

        @foreach (var e in Model.Lista)
        {
            <a href="~/Home/especie/@e.Especie">
                <img src="~/especies/@(e.Id).jpg" title="@e.Especie"/>
            </a>
        }
    </div>


<script>
    var c = document.querySelector("main h1 img").cloneNode();
    document.querySelector("main h1").appendChild(c);
</script>